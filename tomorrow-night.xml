   <!-- JSON   -->
  <style name="JsonNormal" foreground="#282828"/>
  <style name="JsonNumber" foreground="#FFCD22"/>
  <style name="JsonString" foreground="#B5BD68"/>
  <style name="JsonOperator" foreground="#A02DCC"/>
  <style name="JsonIdentifier" foreground="#B294BB"/>

</style-scheme>
