  <style name="PythonVariableValue" foreground="#6d3206"/>

  <!-- Coffee -->
  <style name="CoffeeScriptComment" foreground="#969896"/>
  <style name="CoffeeScriptNumber" foreground="#FFCD22"/>
  <style name="CoffeeScriptString" foreground="#B5BD68"/>
  <style name="CoffeeScriptOperator" foreground="#ce395d"/>
  <style name="CoffeeScriptFuture" foreground="#B294BB"/>
  <style name="CoffeeScriptKeyword" foreground="#B294BB"/>
  <style name="CoffeeScriptFunction" foreground="#DE935F"/>
  <style name="CoffeeScriptRegex" foreground="#FFCD22"/>

  <!-- XML   -->

  <style name="XmlSyntaxChar" foreground="#B294BB" bold="1"/>
  <style name="XmlElementName" foreground="#B294BB" bold="1"/>
  <style name="XmlComment" foreground="#969896"/>
  <style name="XmlAttributeName" foreground="#DE935F"/>
  <style name="XmlAttributeValue" foreground="#B5BD68"/>
  <style name="XmlError" foreground="#cacaca"/>
  <style name="XmlOther" foreground="#B294BB"/>


  <!-- MySql  -->
  <style name="Background" foreground="#00aa00"/>
  <style name="NormalMySql" foreground="#333333"/>
  <style name="MySqlComment" foreground="#969896"/>
  <style name="MySqlNumber" foreground="#d35e2c"/>
  <style name="MySqlString" foreground="#B5BD68"/>
  <style name="MySqlOperator" foreground="#808000"/>
  <style name="MySqlIdentifier" foreground="#00c500"/>
